{
    "name": "intact-client",
    "version": "7.1.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --ssl=true --port=4201 intact",
        "serve": "ng serve --ssl=true --port=4201",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "build-intact-models": "ng build intact-models",
        "build-fx": "ng build joe-fx && ng build joe-types && ng build joe-models && ng build joe-viewmodels && ng build joe-store-api",
        "build-all_old": "rimraf dist/joe-fx && ng build joe-fx && rimraf dist/joe-types && ng build joe-types && rimraf dist/joe-models && ng build joe-models && rimraf dist/joe-viewmodels && ng build joe-viewmodels && rimraf dist/joe-store-api && ng build joe-store-api && rimraf dist/intact-models && ng build intact-models && rimraf dist/intact && ng build intact",
        "build-all": "ng build joe-fx && ng build joe-types && ng build joe-models && ng build joe-viewmodels && ng build joe-store-api && ng build intact-models && ng build intact",
        "build-intact": "ng build intact --delete-output-path=false",
        "build-all-prod": "rimraf dist && ng build joe-fx --prod && ng build joe-types --prod && ng build joe-viewmodels --prod && ng build joe-store-api --prod && ng build intact-models --prod && ng build intact --prod",
        "test:joe-fx": "jest --config ./projects/joe-fx/jest.config.js --runInBand",
        "ngsw": "./node_modules/.bin/ngsw-config ./dist/intact ./ngsw-config.json",
        "test": "ng test",
        "version": "node ./scripts/version.js",
        "rename-beta": "node ./scripts/rename-beta.js",
        "cypress:open": "ng run intact:cypress-open",
        "cypress:open:dev": "ng run intact:cypress-open --config-file projects/intact/cypress/config/cypress.config.ts",
        "cypress:open:beta": "ng run intact:cypress-open --config-file projects/intact/cypress/config/cypress.config.beta.ts",
        "cypress:run": "ng run intact:cypress-run --dev-server-target=''",
        "cypress:run:beta": "ng run intact:cypress-run --config-file projects/intact/cypress/config/cypress.config.beta.ts --dev-server-target=''",
        "cypress:run:next": "ng run intact:cypress-run --config-file projects/intact/cypress/config/cypress.config.next.ts --dev-server-target=''",
        "cypress:install": "cypress install",
        "merge-reports": "jrm projects/intact/cypress/results/combined.xml \"projects/intact/cypress/results/output-*.xml\""
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^16.0.1",
        "@angular/cdk": "^16.1.2",
        "@angular/common": "^16.0.1",
        "@angular/compiler": "^16.0.1",
        "@angular/core": "^16.0.1",
        "@angular/forms": "^16.0.1",
        "@angular/material": "^16.1.2",
        "@angular/material-moment-adapter": "^16.1.2",
        "@angular/platform-browser": "^16.0.1",
        "@angular/platform-browser-dynamic": "^16.0.1",
        "@angular/router": "^16.0.1",
        "@angular/service-worker": "^16.0.1",
        "@antv/g6": "^4.8.10",
        "@cypress/schematic": "^2.5.0",
        "@microsoft/signalr": "^6.0.1",
        "angular-oauth2-oidc": "^13.0.1",
        "chart.js": "^3.8.2",
        "cypress": "^12.16.0",
        "insert-css": "^2.0.0",
        "moment": "^2.29.1",
        "ng2-charts": "^4.0.0",
        "ngx-markdown": "^16.0.0",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.13.0"
    },
    "devDependencies": {
        "@angular-builders/jest": "^16.0.0",
        "@angular-devkit/build-angular": "^16.0.3",
        "@angular/cli": "^16.0.1",
        "@angular/compiler-cli": "^16.0.1",
        "@angular/language-service": "^16.0.1",
        "@okta/okta-auth-js": "^6.2.0",
        "@types/insert-css": "^2.0.1",
        "@types/jest": "^28.1.6",
        "@types/node": "^12.20.47",
        "jest": "^29.5.0",
        "jest-createspyobj": "^2.0.0",
        "jest-environment-jsdom": "^29.5.0",
        "junit-report-merger": "^3.0.6",
        "marked": "^4.0.18",
        "ng-packagr": "^16.0.1",
        "replace-json-property": "^1.8.0",
        "typescript": "^5.0.2"
    },
    "prettier": {
        "printWidth": 100,
        "trailingComma": "none",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true
    }
}
